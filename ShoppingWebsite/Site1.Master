            margin: 0rem 0.5rem;
        }

        .nav2 {
            z-index: 3;
        }

        /* The sticky class is added to the navbar with JS when it reaches its scroll position */
        .sticky {
            position: fixed;
            top: 0;
            width: 100%;
        }
  

        .nav2 .nav-item:hover {
            background-color: #2b2ba4;
            /* font-weight:bold;*/
